    "preview": "svelte-kit preview",
    "postbuild": "npm run gen:seo",
    "gen:seo-artifacts": "node ./scripts/generate-seo-artifacts.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "husky": "^7.0.1",
    "mdsvex": "^0.9.8",
    "postcss": "^8.3.5",
    "postcss-load-config": "^3.1.0",
    "svelte": "^3.38.3",
    "svelte-preprocess": "^4.7.3",
    "tailwindcss": "^2.2.4"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run gen:seo-artifacts && git add ./static"
    }
  }
}
